import junit.framework.Test;
import junit.framework.TestSuite;

public class Desafio4_teste {

  public static Test suite() {
    TestSuite suite = new TestSuite();
    suite.addTestSuite(Editar funcionario - dados validos.class);
    suite.addTestSuite(Listar funcionario por setor.class);
    suite.addTestSuite(Editar Setor - dados validos.class);
    suite.addTestSuite(Consultar escala por setor.class);
    suite.addTestSuite(Marcar horário.class);
    suite.addTestSuite(Aceitar Sugestão.class);
    return suite;
  }

  public static void main(String[] args) {
    junit.textui.TestRunner.run(suite());
  }
}
